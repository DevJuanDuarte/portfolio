---
import EstudiosItem from "./EstudiosItem.astro";
const ESTUDIOS = [
  {
    date:'2020 - 2022',
    date2:'Servicio Nacional de Aprendizaje (SENA).',
    title:'Tegnología en Ánalisis y Desarrollo de Sistemas de Información',
    description:' PHP · JavaScript · TypeScript · Angular · Slim · Java · PostgreSQL · MySQL · Bootstrap · Diseño UI/UX',
    link:'https://drive.google.com/file/d/1E5JU3oPxWzlHrT5PXWts7siU7ZTcHNYT/view?usp=sharing'
  }
]
---


<ol class="relative border-s border-gray-200 ml-3.5">
  {
    ESTUDIOS.map(estudios => (
    <li class="mb-10 ms-4">
      <EstudiosItem {...estudios}/>  
    </li>
    ))
  }
</ol>


